@model IEnumerable<online_test.Models.Canditate_info>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayoutPage.cshtml";
}

<h2>CANDITATE-INFO</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<div class="row">
    <div class="col-12">
        <div class="card my-4">
            <div class="card-header p-0 position-relative mt-n4 mx-3 z-index-2">
                <div class=" bg-gradient-dark border-radius-lg pt-4 pb-3">
                    <h6 class="text-white text-capitalize ps-3">Canditate table</h6>
                </div>
            </div>
            <div class="card-body px-0 pb-2">
                <div class="table-responsive p-0">
                    <table class="table align-items-center mb-0">
                        <tr>
                            <th class="text-uppercase text-secondary text-s font-weight-bolder">
                                @Html.DisplayName("Name")
                            </th>
                            <th class="text-uppercase text-secondary text-s font-weight-bolder">
                                @Html.DisplayName("Email")
                            </th>
                            <th class="text-uppercase text-secondary text-s font-weight-bolder">
                                @Html.DisplayName("DOB")
                            </th>
                            <th class="text-uppercase text-secondary text-s font-weight-bolder">
                                @Html.DisplayName("Gender")
                            </th>
                            <th class="text-uppercase text-secondary text-s font-weight-bolder">
                                @Html.DisplayName("Password")
                            </th>
                            <th class="text-uppercase text-secondary text-s font-weight-bolder">
                                @Html.DisplayName("Canditate_info")
                            </th>
                            <th class="text-uppercase text-secondary text-s font-weight-bolder">
                                @Html.DisplayName("CNIC")
                            </th>
                            <th class="text-uppercase text-secondary text-s font-weight-bolder">
                                @Html.DisplayName("Edu_tbl")
                            </th>
                            <th class="text-uppercase text-secondary text-s font-weight-bolder">
                                @Html.DisplayName("work_tbl")
                            </th>
                            <th>
                            </th>
                        </tr>

                        @foreach (var item in Model)
                        {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.Name)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Email)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Cell_no)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.DOB)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Gender)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Password)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.CNIC)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Edu_tbl.Id)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.work_tbl.Id)
                        </td>
                        <td>
                            @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
                            @Html.ActionLink("Details", "Details", new { id = item.Id }) |
                            @Html.ActionLink("Delete", "Delete", new { id = item.Id })
                        </td>
                    </tr>
                }

                    </table>
                </div>
            </div>
        </div>
    </div>
</div>


